{
    "name": "thekeymaster/asurm",
    "description": "Asurm - Findologic Account API Client",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Brader",
            "email": "dominik@brader.co.at",
            "role": "Maintainer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FINDOLOGIC\\Asurm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FINDOLOGIC\\Asurm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml.dist --coverage-text --coverage-clover=coverage.xml"
        ]
    }
}
